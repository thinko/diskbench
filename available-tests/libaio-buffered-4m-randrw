[global]
description=4M blocks (Buffered I/O and Sync on Finish)

[libaio-buffered-sync-4m-randrw]
bs=4m
rw=randrw
direct=0
end_fsync=1
size=${TEST_SIZE}
ioengine=libaio
iodepth=1
filename=${TEST_FILENAME}
directory=${TEST_DIRECTORY}
runtime=${RUNTIME}
exec_prerun=bash -c 'sync && echo 3 >/proc/sys/vm/drop_caches'
