[global]
description=4M blocks (Non-buffered I/O Sequential)

[libaio-not-buffered-4m-rw]
bs=4m
rw=rw
direct=1
size=${TEST_SIZE}
ioengine=libaio
iodepth=${IO_DEPTH}
filename=${TEST_FILENAME}
directory=${TEST_DIRECTORY}
runtime=${RUNTIME}
exec_prerun=bash -c 'sync && echo 3 >/proc/sys/vm/drop_caches'
