[global]
description=Sync engine 4k blocks (Non-buffered I/O) Read-only

[sync-not-buffered-4k-r]
bs=4k
rw=read
direct=1
buffered=0
size=${TEST_SIZE}
ioengine=sync
iodepth=${IO_DEPTH}
filename=${TEST_FILENAME}
directory=${TEST_DIRECTORY}
runtime=${RUNTIME}exec_prerun=bash -c 'sync && echo 3 >/proc/sys/vm/drop_caches'
